    dup length 0 ne {
	carresize posx carresize sub posy begints
	(0.05 w 1 0 0 rg 1 0 0 RG\n)out
	(0 0 m 1 -1 l S\n)out
	(1 -1 m 0.8 -0.7 l 0.85 -0.85 l 0.7 -0.8 l h f\n)out
	(q 0.71 -0.71 0.71 0.71 0 0 cm\n) out
	0.3 0 0.6 0.4 (FontArialNarrow) centertext
	(Q\n) out
	endts
    }{pop}ifelse
}def

